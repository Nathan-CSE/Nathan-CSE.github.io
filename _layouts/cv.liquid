---
layout: default
---
<div class="post">
    <header class="post-header">
      <h1 class="post-title">
        {{ page.title }}
      </h1>
      <h2>  
        
      </h2>  
      {% if page.description %}
        <p class="post-description">{{ page.description }}</p>
      {% endif %}
    </header>

  <article>
    {{ content }}
    <iframe src="/assets/pdf/nathan_nguyen_resume.pdf" width="100%" height="1000px"></iframe>
  </article>

  {% if page.related_publications %}
    <h2>References</h2>
    <div class="publications">
      {% bibliography --cited_in_order %}
    </div>
  {% endif %}

  {% if site.giscus and page.giscus_comments %}
    {% include giscus.liquid %}
  {% endif %}
</div>

